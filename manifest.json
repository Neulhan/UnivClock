{
  "name": "한양대학교 종강시계",
  "version": "1.3",
  "manifest_version": 2,
  "description": "한양대학교 종강까지 남은 시간을 알려주는 시계입니다.",
  "content_security_policy": "script-src 'self' https://hyclock.hanyang.life/; object-src 'self'",
  "permissions": ["tabs", "storage"],
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.jpg"
  },
  "browser_action": {
    "default_title": "한양대학교 종강시계"
  },
  "chrome_url_overrides": {
    "newtab": "public/index.html"
  }
}
